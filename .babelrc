{
  "presets": ["react-app"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "components": "./src/components",
          "contexts": "./src/contexts",
          "pages": "./src/pages",
          "utils": "./src/utils",
          "db": "./src/database.js",
          "theme": "./src/theme.scss"
        },
        "extensions": [".js", ".json"]
      }
    ]
  ]
}
