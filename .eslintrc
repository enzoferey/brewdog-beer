{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-console": 1,
    "no-unused-vars": 1,
    "react/display-name": 0
  },
  "globals": {
    "process": false,
    "global": false,
    "expect": false,
    "test": false
  }
}
